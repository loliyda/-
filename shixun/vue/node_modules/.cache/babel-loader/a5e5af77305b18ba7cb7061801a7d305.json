{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"PreView\",\n\n  data() {\n    return {\n      tableData: [],\n      form: {}\n    };\n  },\n\n  methods: {\n    predict(result) {\n      window.location.href = result;\n    },\n\n    preview() {\n      this.form.uploadTime = new Date().getTime();\n      request.post(\"/api/preview\", this.form).then(res => {\n        console.log(res);\n        this.load();\n      });\n    },\n\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.file = res.data;\n    },\n\n    load() {\n      request.get(\"/api/preview\").then(res => {\n        console.log(res);\n        this.tableData = res.data;\n      });\n    }\n\n  },\n\n  created() {\n    this.load();\n  }\n\n};","map":{"version":3,"mappings":"AAwDA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,SADO;;EAEbC,IAAI,GAAE;IACJ,OAAM;MACJC,SAAS,EAAC,EADN;MAEJC,IAAI,EAAC;IAFD,CAAN;EAID,CAPY;;EAQbC,OAAO,EAAC;IACNC,OAAO,CAACC,MAAD,EAAQ;MACbC,MAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAqBH,MAArB;IACD,CAHK;;IAINI,OAAO,GAAE;MACP,KAAKP,IAAL,CAAUQ,UAAV,GAAuB,IAAIC,IAAJ,GAAWC,OAAX,EAAvB;MACAd,OAAO,CAACe,IAAR,CAAa,cAAb,EAA4B,KAAKX,IAAjC,EAAuCY,IAAvC,CAA4CC,GAAE,IAAI;QAChDC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKG,IAAL;MACD,CAHD;IAID,CAVK;;IAWNC,kBAAkB,CAACJ,GAAD,EAAK;MACrBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKb,IAAL,CAAUkB,IAAV,GAAiBL,GAAG,CAACf,IAArB;IACD,CAdK;;IAeNkB,IAAI,GAAE;MACJpB,OAAO,CAACuB,GAAR,CAAY,cAAZ,EAA4BP,IAA5B,CAAiCC,GAAE,IAAI;QACrCC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKd,SAAL,GAAec,GAAG,CAACf,IAAnB;MACD,CAHD;IAIF;;EApBM,CARK;;EA8BbsB,OAAO,GAAG;IACR,KAAKJ,IAAL;EACF;;AAhCa,CAAf","names":["request","name","data","tableData","form","methods","predict","result","window","location","href","preview","uploadTime","Date","getTime","post","then","res","console","log","load","filesUploadSuccess","file","get","created"],"sourceRoot":"","sources":["C:\\Users\\83843\\Desktop\\实训Ⅲ\\shixun\\vue\\src\\views\\PreView.vue"],"sourcesContent":["<templat