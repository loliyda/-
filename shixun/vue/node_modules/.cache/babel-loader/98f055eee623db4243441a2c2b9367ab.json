{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"LoginView\",\n\n  data() {\n    return {\n      form: {},\n      LOGIN: true,\n      REGISTER: false,\n      FORGET: false\n    };\n  },\n\n  methods: {\n    Login() {\n      request.post(\"/api/user/login\", this.form).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"登录成功\"\n          });\n          sessionStorage.setItem(\"user\", JSON.stringify(res.data));\n          this.$router.push(\"/layout\");\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    Register() {\n      if (this.form.password !== this.form.confirm) {\n        alert(\"两次密码不一致\");\n      } else {\n        request.post(\"/api/user/register\", this.form).then(res => {\n          if (res.code === '0') {\n            alert(\"注册成功\");\n            this.FORGET = false;\n            this.REGISTER = false;\n            this.LOGIN = true;\n          } else {\n            alert(res.msg);\n          }\n        });\n      }\n\n      this.form = {};\n    },\n\n    revise() {\n      request.put(\"/api/user/revise\", this.form).then(res => {\n        // console.log(res)\n        if (res.code === '0') {\n          alert(\"修改成功\");\n          this.FORGET = false;\n          this.REGISTER = false;\n          this.LOGIN = true;\n        } else {\n          alert(res.msg);\n        }\n\n        this.form = {};\n      });\n    },\n\n    ForgetPassword() {\n      this.FORGET = true;\n      this.REGISTER = false;\n      this.LOGIN = false;\n    },\n\n    NewUser() {\n      this.FORGET = false;\n      this.REGISTER = true;\n      this.LOGIN = false;\n    },\n\n    Return() {\n      this.FORGET = false;\n      this.REGISTER = false;\n      this.LOGIN = true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"